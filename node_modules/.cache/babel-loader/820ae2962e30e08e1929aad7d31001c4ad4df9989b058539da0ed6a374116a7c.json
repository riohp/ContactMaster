v\">\r\n      <li class=\"nav-item\">\r\n        <sidenav-item\r\n          to=\"/dashboard-default\"\r\n          :class=\"obtenerRuta() === 'dashboard-default' ? 'active' : ''\"\r\n          navText=\"Panel de Control\"\r\n        >\r\n          <template v-slot:icon>\r\n            <i class=\"ni ni-tv-2 text-primary text-sm opacity-10\"></i>\r\n          </template>\r\n        </sidenav-item>\r\n      </li>\r\n\r\n      <li class=\"nav-item\">\r\n        <sidenav-item\r\n          to=\"/tables\"\r\n          :class=\"obtenerRuta() === 'tables' ? 'active' : ''\"\r\n          navText=\"Tablas\"\r\n        >\r\n          <template v-slot:icon>\r\n            <i class=\"ni ni-calendar-grid-58 text-warning text-sm opacity-10\"></i>\r\n          </template>\r\n        </sidenav-item>\r\n      </li>\r\n\r\n\r\n      <li class=\"mt-3 nav-item\">\r\n        <h6\r\n          class=\"text-xs ps-4 text-uppercase font-weight-bolder opacity-6\"\r\n          :class=\"esRTL ? 'me-4' : 'ms-2'\"\r\n        >\r\n          PÁGINAS DE CUENTA\r\n        </h6>\r\n      </li>\r\n\r\n      <li class=\"nav-item\">\r\n        <sidenav-item\r\n          to=\"/signin\"\r\n          :class=\"obtenerRuta() === 'signin' ? 'active' : ''\"\r\n          navText=\"Iniciar Sesión\"\r\n        >\r\n          <template v-slot:icon>\r\n            <i class=\"ni ni-single-copy-04 text-danger text-sm opacity-10\"></i>\r\n          </template>\r\n        </sidenav-item>\r\n      </li>\r\n\r\n      <li class=\"nav-item\">\r\n        <sidenav-item\r\n          to=\"/signup\"\r\n          :class=\"obtenerRuta() === 'signup' ? 'active' : ''\"\r\n          navText=\"Registrarse\"\r\n        >\r\n          <template v-slot:icon>\r\n            <i class=\"ni ni-collection text-info text-sm opacity-10\"></i>\r\n          </template>\r\n        </sidenav-item>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n\r\n  <div class=\"pt-3 mx-3 mt-3 sidenav-footer\">\r\n    <sidenav-card\r\n      :card=\"{\r\n        title: '¿N